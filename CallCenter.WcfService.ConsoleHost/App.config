<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/>
	</startup>
	<system.serviceModel>
		<services>
			<service name="CallCenter.WcfService.LoginService">
				<endpoint address="net.tcp://localhost:8009/LoginService"
                  binding="netTcpBinding"
                  contract="CallCenter.ServiceContracts.Services.ILoginService" />
			</service>
			<service name="CallCenter.WcfService.ChatService">
				<endpoint address="net.tcp://localhost:8009/ChatService"
                  binding="netTcpBinding"
                  contract="CallCenter.ServiceContracts.Services.IChatService" />
			</service>
		</services>
	</system.serviceModel>
</configuration>
